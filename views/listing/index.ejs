<% layout("layouts/boilerplate") %>

<h3>All Listings</h3>

<form method="get" action="/listings/new">
  <button type="submit">New Listing</button>
</form>

<ul>
  <% for (let listing of allistings) { %>
    <li>
      <a href="/listings/<%= listing._id %>">
        <%= listing.title %>
      </a>
    </li>
  <% } %>
</ul>


